<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.dao.mapper.FollowsMapper">
    <select id="getFollowsByUserid" resultType="com.example.demo.domain.entity.Follows">
        select *
        from Follows
        where userid = #{userid}
    </select>
    <select id="getFollowsByFollowerUserid" resultType="com.example.demo.domain.entity.Follows">
        select *
        from Follows
        where follower_userid = #{follower_userid}
    </select>
</mapper>